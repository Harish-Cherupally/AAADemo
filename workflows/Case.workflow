        <fullName>Transactional Survey Send</fullName>
        <active>false</active>
        <formula>IsClosed = TRUE
&amp;&amp;
(
ISBLANK( Contact.Date_Transactional_Survey_Last_Sent__c )

||

((TODAY() - Contact.Date_Transactional_Survey_Last_Sent__c) &gt; 10))
&amp;&amp;
NOT(ISPICKVAL(Category__c, &quot;Cancel Service&quot;))
&amp;&amp;
NOT(ISPICKVAL(Category__c, &quot;Collections&quot;))
&amp;&amp;
NOT(ISPICKVAL(Category__c, &quot;Data Entry&quot;))
&amp;&amp;
NOT(ISPICKVAL(Category__c, &quot;Service Quality&quot;))
&amp;&amp;
NOT(ISPICKVAL(Category__c, &quot;Automated Email&quot;))
&amp;&amp;
NOT(ISBLANK(ContactId))</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Check_Send_Survey_Checkbox</name>
                <type>FieldUpdate</type>
            </actions>
            <timeLength>7</timeLength>
            <workflowTimeTriggerUnit>Days</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
