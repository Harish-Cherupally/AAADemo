<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>51.0</apiVersion>
    <formulas>
        <name>Discount_Dollars</name>
        <dataType>Currency</dataType>
        <expression>(({!$Record.UnitPrice} * {!$Record.QuantityConsumed}) * {!$Record.WorkOrder.Opportunity__r.Equipment_Discount__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Final_Price</name>
        <dataType>Currency</dataType>
        <expression>(({!$Record.UnitPrice} * (1-{!$Record.WorkOrder.Opportunity__r.Equipment_Discount__c})) * {!$Record.QuantityConsumed}) +
((({!$Record.UnitPrice} * (1-{!$Record.WorkOrder.Opportunity__r.Equipment_Discount__c})) * {!$Record.QuantityConsumed}) * {!$Record.WorkOrder.Opportunity__r.Tax_Percent__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Sales_Price</name>
        <dataType>Currency</dataType>
        <expression>{!$Record.UnitPrice} * (1-{!$Record.WorkOrder.Opportunity__r.Equipment_Discount__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Tax_Dollars</name>
        <dataType>Currency</dataType>
        <expression>((({!$Record.UnitPrice}* (1-{!$Record.WorkOrder.Opportunity__r.Equipment_Discount__c})) * {!$Record.QuantityConsumed}) * {!$Record.WorkOrder.Opportunity__r.Tax_Percent__c})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Total_Price</name>
        <dataType>Currency</dataType>
        <expression>(({!$Record.UnitPrice} * (1-{!$Record.WorkOrder.Opportunity__r.Equipment_Discount__c})) * {!$Record.QuantityConsumed})</expression>
        <scale>2</scale>
    </formulas>
    <interviewLabel>Products Consumed Calculations {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Products Consumed Calculations</label>
    <processMetadataValues>
