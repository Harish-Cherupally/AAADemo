                </rightValue>
            </conditions>
            <connector>
                <targetReference>OnHoldPerCustomer2</targetReference>
            </connector>
            <label>OnHoldPerCustomer</label>
        </rules>
        <rules>
            <name>Departed</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>serviceAppointmentCurrentData.Status</leftValueReference>
                <operator>EqualTo</operator>
